<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <FrameLayout
        android:id="@+id/ferdigContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MusikkQuizFerdigFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/GratulasjonsTxt"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="26dp"
                android:layout_marginBottom="114dp"
                android:gravity="center"
                android:text="Gratulerer!"
                android:textSize="24sp"
                app:layout_constraintBottom_toTopOf="@+id/imageView9"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/generertTxt"
                android:layout_width="188dp"
                android:layout_height="0dp"
                android:layout_marginTop="103dp"
                android:layout_marginBottom="270dp"
                android:gravity="center"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/backBtn"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/resultatTxt"
                android:layout_width="132dp"
                android:layout_height="0dp"
                android:layout_marginBottom="19dp"
                android:gravity="center"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/backBtn"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView9" />

            <Button
                android:id="@+id/backBtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="95dp"
                android:text="Tilbake til meny"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/generertTxt" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="17dp"
                android:layout_marginBottom="23dp"
                android:src="@drawable/trophy"
                app:layout_constraintBottom_toTopOf="@+id/resultatTxt"
                app:layout_constraintEnd_toEndOf="@+id/resultatTxt"
                app:layout_constraintTop_toBottomOf="@+id/GratulasjonsTxt" />
        </androidx.constraintlayout.widget.ConstraintLayout>



    </FrameLayout>

</layout>
