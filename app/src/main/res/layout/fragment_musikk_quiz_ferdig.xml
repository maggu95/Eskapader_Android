<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <FrameLayout
        android:id="@+id/ferdigContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MusikkQuizFerdigFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/GratulasjonsTxt"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="26dp"
                android:layout_marginBottom="357dp"
                android:gravity="center"
                android:text="Gratulerer!"
                android:textSize="24sp"
                app:layout_constraintBottom_toTopOf="@+id/backBtn"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="188dp"
                android:layout_height="0dp"
                android:layout_marginBottom="75dp"
                android:gravity="center"
                android:text="Du har gjennomfÃ¸rt Musikk quizen!"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/resultatTxt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/GratulasjonsTxt" />

            <TextView
                android:id="@+id/resultatTxt"
                android:layout_width="132dp"
                android:layout_height="0dp"
                android:layout_marginBottom="250dp"
                android:gravity="center"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView21" />

            <Button
                android:id="@+id/backBtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="94dp"
                android:text="Tilbake til meny"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/GratulasjonsTxt" />
        </androidx.constraintlayout.widget.ConstraintLayout>



    </FrameLayout>

</layout>
