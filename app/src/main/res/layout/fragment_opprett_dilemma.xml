<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".OpprettMusikkQuizFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/dilemma_Alt2"
            android:layout_width="351dp"
            android:layout_height="67dp"
            android:ems="10"
            android:hint="Alternativ 2"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="295dp" />

        <EditText
            android:id="@+id/dilemma_Spm"
            android:layout_width="351dp"
            android:layout_height="67dp"
            android:ems="10"
            android:hint="Spørsmålstekst her"
            android:inputType="textPersonName"
            android:textAlignment="center"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="71dp" />

        <EditText
            android:id="@+id/dilemma_Alt1"
            android:layout_width="351dp"
            android:layout_height="66dp"
            android:ems="10"
            android:hint="Alternativ 1"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="198dp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Legg til"
            tools:layout_editor_absoluteX="155dp"
            tools:layout_editor_absoluteY="447dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>
