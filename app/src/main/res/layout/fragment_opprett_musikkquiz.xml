<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".OpprettMusikkQuizFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/inputQuizNavn"
            android:layout_width="183dp"
            android:layout_height="49dp"
            android:layout_marginTop="42dp"
            android:layout_marginBottom="44dp"
            android:ems="10"
            android:gravity="center"
            android:hint="Min musikkquiz"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/inputSangtekst"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/inputSangtekst"
            android:layout_width="233dp"
            android:layout_height="101dp"
            android:layout_marginTop="135dp"
            android:ems="10"
            android:gravity="center"
            android:hint="Mama just killed a man"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/inputAlt1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="319dp"
            android:layout_marginEnd="59dp"
            android:layout_marginBottom="78dp"
            android:ems="10"
            android:gravity="center"
            android:hint="Alternativ 1"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/inputAlt3"
            app:layout_constraintEnd_toStartOf="@+id/inputAlt2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/inputAlt3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="59dp"
            android:layout_marginBottom="110dp"
            android:ems="10"
            android:gravity="center"
            android:hint="Alternativ 3"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/inputAlt4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/inputAlt1"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/inputAlt4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="110dp"
            android:ems="10"
            android:gravity="center"
            android:hint="Alternativ 4"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/inputAlt3"
            app:layout_constraintTop_toBottomOf="@+id/inputAlt2" />

        <EditText
            android:id="@+id/inputAlt2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="319dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="78dp"
            android:ems="10"
            android:gravity="center"
            android:hint="Alternativ 2"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/inputAlt4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/inputAlt1"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btBekreftQuiz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="41dp"
            android:layout_marginBottom="34dp"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView22" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="61dp"
            android:layout_height="27dp"
            android:layout_marginEnd="61dp"
            android:layout_marginBottom="46dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>
